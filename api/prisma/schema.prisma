generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Wallet {
  id        String   @id @default(uuid())
  address   String   @unique
  balance   Decimal  @default(0)
  publicKey String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("wallets")
}
